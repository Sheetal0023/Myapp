        <div class="list" id="append">
            {{#each amcs}}
             <form action="/getamcdetails" method="post">
                <div class="listValue">
                    <div class="amcHead">
                        <div class="head One">{{this.device_name}}</div>
                        <div class="head Two">
                            ₹{{this.contract_charges}}/-
                            <div class="contract">{{this.contract_period}}</div>
                        </div>
                    </div>
                    <hr>
                    <div class="amcContent">
                        <div class="amcData">
                            <div class="first">Device category</div>
                            <div class="second">{{this.device_cetagory}}</div>
                        </div>
                        <div class="amcData">
                            <div class="first">{{this.maintenance_period_one}}</div>
                            <div class="second">₹{{this.charges_one}}</div>
                        </div>
                        <div class="amcData">
                            <div class="first">{{this.maintenance_period_two}}</div>
                            <div class="second">₹{{this.charges_two}}</div>
                        </div>
                        <div class="amcData">
                            <div class="first">{{this.maintenance_period_three}}</div>
                            <div class="second">₹{{this.charges_three}}</div>
                        </div>
                        <div class="amcData">
                            <div class="first">{{this.maintenance_period_four}}</div>
                            <div class="second">₹{{this.charges_four}}</div>
                        </div>
                        
                    </div>
                    <div class="form">
                        <input type="text" name="id" value="{{this._id}}" hidden>
                        <button type="submit" class="showBtn"> Show More</button>
                    </div>
                    
                </div>
                
                </form>
            {{/each}}
